{
  "extends": [
    "./node_modules/gts/",
    "plugin:react/recommended",
    "prettier"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": ["react", "react-hooks", "prettier"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "react-hooks/exhaustive-deps": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}